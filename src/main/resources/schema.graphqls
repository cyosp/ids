type Query {
    list: [Image]
    users: [User]
}

type Image  {
    id: ID
    name: String
    urlPath: String
    previewUrlPath: String
    thumbnailUrlPath: String
}

enum Role {
    ADMINISTRATOR
    VIEWER
}

type User {
    id: ID
    login: String
    password: String
    role: Role
}

type Mutation {
    generateAlternativeFormats: [Image]
}